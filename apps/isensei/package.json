{
  "name": "isensei-blog",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "yarn run clean && TAILWIND_MODE=watch next dev -p 3000",
    "build": "prisma generate && prisma migrate deploy && next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "echo 'Running testing suite'",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts && prettier --write .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "pscale:shadow": "pscale connect isensei shadow --port 3310",
    "pscale:dev": "pscale connect isensei dev --port 3309",
    "pscale:main": "pscale connect isensei main --port 3309",
    "compose": "node ./scripts/compose.js"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.7.4",
    "@codesandbox/sandpack-react": "^1.5.5",
    "@headlessui/react": "^1.6.6",
    "@isensei/ui": "*",
    "@mailchimp/mailchimp_marketing": "^3.0.78",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@types/react-dom": "17.0.2",
    "classnames": "^2.3.1",
    "esbuild": "^0.15.7",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "js-yaml": "^4.1.0",
    "mdx-bundler": "^8.0.1",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.0",
    "next-translate": "^1.5.0",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "0.4.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "^6.0.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.30.7",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@isensei/tailwind-config": "*",
    "@isensei/tsconfig": "*",
    "@next/bundle-analyzer": "12.2.5",
    "@svgr/webpack": "^6.3.1",
    "@types/react": "17.0.2",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint-preset-isensei": "*",
    "file-loader": "^6.2.0",
    "globby": "^11.0.4",
    "inquirer": "^8.2.0",
    "next-remote-watch": "^1.0.0",
    "prisma": "4.3.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  }
}
