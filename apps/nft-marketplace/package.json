{
  "name": "nft-marketplace",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "yarn run clean && TAILWIND_MODE=watch next dev",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "echo 'Running testing suite'",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts && prettier --write .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .next",
    "pscale:shadow": "pscale connect isensei shadow --port 3310",
    "pscale:dev": "pscale connect isensei dev --port 3309",
    "pscale:main": "pscale connect isensei main --port 3309"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.4.2",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.2",
    "ipfs-http-client": "50.1.2",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "eslint-config-isensei": "*",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15"
  }
}
