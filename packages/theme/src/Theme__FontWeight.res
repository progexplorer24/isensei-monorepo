type t = [#100 | #200 | #300 | #400 | #500 | #600 | #700 | #800 | #900]

let toValue = fontWeight =>
  switch fontWeight {
  | #100 => #num(100)
  | #200 => #num(200)
  | #300 => #num(300)
  | #400 => #num(400)
  | #500 => #num(500)
  | #600 => #num(600)
  | #700 => #num(700)
  | #800 => #num(800)
  | #900 => #num(900)
  }
